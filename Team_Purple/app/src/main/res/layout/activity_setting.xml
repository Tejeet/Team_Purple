<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingActivity">

    <TextView
        android:id="@+id/tvToolBarTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_12"
        android:paddingStart="@dimen/dp_20"
        android:paddingTop="@dimen/dp_20"
        android:text="@string/text_AppSetting"
        android:background="@color/black"
        android:padding="@dimen/dp_20"
        android:textSize="@dimen/sp_22"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_12"
        android:background="@drawable/ic_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_Community"
        android:id="@+id/tvCoummnity"
        app:layout_constraintTop_toBottomOf="@+id/tvToolBarTitle"
        android:textSize="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_40"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvCoummnityguidelines"
        app:layout_constraintTop_toBottomOf="@+id/tvCoummnity"
        android:text="@string/text_CommunityGuidelines"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <ImageView
        android:id="@+id/arrow1"
        android:layout_width="wrap_content"
        android:gravity="center_vertical"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_arrow1"
        app:layout_constraintTop_toTopOf="@+id/tvCoummnityguidelines"
        app:layout_constraintBottom_toBottomOf="@+id/tvCoummnityguidelines"
        app:layout_constraintEnd_toEndOf="@+id/tvCoummnityguidelines" />


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvSafetytips"
        app:layout_constraintTop_toBottomOf="@+id/tvCoummnityguidelines"
        android:text="@string/text_SafetyTips"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <TextView
        android:id="@+id/tvLegal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_Legal"
        app:layout_constraintTop_toBottomOf="@+id/tvSafetytips"
        android:textSize="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvPrivacy"
        app:layout_constraintTop_toBottomOf="@+id/tvLegal"
        android:text="@string/text_PrivacyPolicy"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvTermsofservice"
        app:layout_constraintTop_toBottomOf="@+id/tvPrivacy"
        android:text="@string/text_Termsofservice"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvLiceness"
        app:layout_constraintTop_toBottomOf="@+id/tvTermsofservice"
        android:text="@string/text_Licenses"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_SelectLanguage"
        android:id="@+id/tvSelect"
        app:layout_constraintTop_toBottomOf="@+id/tvLiceness"
        android:textSize="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvEnglish"
        app:layout_constraintTop_toBottomOf="@+id/tvSelect"
        android:text="@string/text_English"
        android:textAllCaps="false"
        android:gravity="start|center_vertical"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvLogout"
        app:layout_constraintTop_toBottomOf="@+id/tvEnglish"
        android:text="@string/text_Logout"
        android:textAllCaps="false"
        android:gravity="center"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvDelete"
        app:layout_constraintTop_toBottomOf="@+id/tvLogout"
        android:text="@string/text_Deleteaccount"
        android:textAllCaps="false"
        android:gravity="center"
        android:paddingLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/rectangle"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20" />

</androidx.constraintlayout.widget.ConstraintLayout>